## Assiomi di Classe
iso17025:CompetentLaboratory a owl:Class ;
    rdfs:label "Laboratorio Competente"@it ;
    rdfs:label "Competent Laboratory"@en ;
    rdfs:comment "Un laboratorio che soddisfa tutti i requisiti ISO 17025"@it ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            iso17025:Laboratory
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasImpartialityRisk ;
              owl:allValuesFrom iso17025:ImpartialityRiskMitigation ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasConfidentialityAgreement ;
              owl:minCardinality "1"^^xsd:nonNegativeInteger ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasCompetence ;
              owl:someValuesFrom iso17025:PersonnelCompetence ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:usesEquipment ;
              owl:allValuesFrom iso17025:CalibratedEquipment ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasMethod ;
              owl:allValuesFrom iso17025:ValidatedMethod ]
        )
    ] .

iso17025:CalibratedEquipment a owl:Class ;
    rdfs:label "Dotazione Tarata"@it ;
    rdfs:comment "Dotazione con taratura valida"@it ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            iso17025:Equipment
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasCalibrationStatus ;
              owl:hasValue "Valid" ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasCalibrationDate ;
              owl:someValuesFrom xsd:date ]
        )
    ] .

iso17025:ValidatedMethod a owl:Class ;
    rdfs:label "Metodo Validato"@it ;
    rdfs:comment "Metodo che è stato validato"@it ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            iso17025:Method
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:isValidated ;
              owl:hasValue "true"^^xsd:boolean ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:validatedBy ;
              owl:someValuesFrom iso17025:MethodValidation ]
        )
    ] .

iso17025:CompleteReport a owl:Class ;
    rdfs:label "Rapporto Completo"@it ;
    rdfs:comment "Rapporto che contiene tutti gli elementi richiesti"@it ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            iso17025:Report
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasReportNumber ;
              owl:cardinality "1"^^xsd:nonNegativeInteger ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasIssueDate ;
              owl:cardinality "1"^^xsd:nonNegativeInteger ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasMethod ;
              owl:minCardinality "1"^^xsd:nonNegativeInteger ]
            [ rdf:type owl:Restriction ;
              owl:onProperty iso17025:hasUncertainty ;
              owl:someValuesFrom iso17025:MeasurementUncertainty ]
        )
    ] .


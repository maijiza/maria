    - Modello completo del dominio geotecnico AGS4
    - Regole di validazione e conformità dettagliate
    - Mappature complete tra domini"""@it .

# ============================================================================
# CLAUSOLA 1-3: SCOPO, RIFERIMENTI NORMATIVI E TERMINI
# ============================================================================

## Concetti Fondamentali ISO 17025
iso17025:ISO17025Standard a owl:Class ;
    rdfs:label "Norma ISO/IEC 17025:2018"@it ;
    rdfs:label "ISO/IEC 17025:2018 Standard"@en ;
    rdfs:comment "La norma internazionale che specifica i requisiti generali per la competenza dei laboratori di prova e taratura"@it ;
    rdfs:subClassOf bfo:InformationContentEntity ;
    rdfs:seeAlso <https://www.iso.org/standard/66912.html> .

iso17025:CompetenceRequirement a owl:Class ;
    rdfs:label "Requisito di Competenza"@it ;
    rdfs:label "Competence Requirement"@en ;
    rdfs:comment "Classe astratta per tutti i requisiti definiti in ISO 17025:2018"@it ;
    rdfs:subClassOf bfo:Disposition ;
    owl:disjointWith iso17025:NonCompetenceRequirement .

iso17025:NonCompetenceRequirement a owl:Class ;
    rdfs:label "Requisito Non di Competenza"@it ;
    rdfs:comment "Requisiti non direttamente correlati alla competenza tecnica"@it .

## Definizioni Chiave (Clausola 3)
iso17025:Laboratory a owl:Class ;
    rdfs:label "Laboratorio"@it ;
    rdfs:label "Laboratory"@en ;
    rdfs:comment "Organismo che esegue una o più fra le seguenti attività: prova, taratura, campionamento associato a successiva prova o taratura"@it ;
    rdfs:subClassOf org:Organization ;
    skos:notation "3.6" .

iso17025:LaboratoryActivity a owl:Class ;
    rdfs:label "Attività di Laboratorio"@it ;
    rdfs:label "Laboratory Activity"@en ;
    rdfs:comment "Prova, taratura o campionamento associato a successiva prova o taratura"@it ;
    skos:notation "3.6 Nota 1" ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:unionOf (
            iso17025:Testing
            iso17025:Calibration
            iso17025:SamplingActivity
        )
    ] .

iso17025:Testing a owl:Class ;
    rdfs:label "Prova"@it ;
    rdfs:label "Testing"@en ;
    rdfs:subClassOf iso17025:LaboratoryActivity ;
    rdfs:comment "Determinazione di una o più caratteristiche secondo una procedura"@it .

iso17025:Calibration a owl:Class ;
    rdfs:label "Taratura"@it ;
    rdfs:label "Calibration"@en ;
    rdfs:subClassOf iso17025:LaboratoryActivity ;
    rdfs:comment "Operazione che stabilisce una relazione tra i valori indicati da uno strumento e i valori corrispondenti di grandezze realizzati da campioni"@it ;
    owl:equivalentClass omt:CalibrationProcess .

iso17025:SamplingActivity a owl:Class ;
    rdfs:label "Attività di Campionamento"@it ;
    rdfs:label "Sampling Activity"@en ;
    rdfs:subClassOf iso17025:LaboratoryActivity ;
    rdfs:comment "Campionamento associato a successiva prova o taratura"@it ;
    owl:equivalentClass sosa:Sampling .

iso17025:Verification a owl:Class ;
    rdfs:label "Verifica"@it ;
    rdfs:label "Verification"@en ;
    rdfs:comment "Messa a disposizione dell'evidenza oggettiva che un dato elemento soddisfa uno o più requisiti specificati"@it ;
    skos:notation "3.8" ;
    rdfs:subClassOf bfo:Process .

iso17025:Validation a owl:Class ;
    rdfs:label "Validazione"@it ;
    rdfs:label "Validation"@en ;
    rdfs:comment "Verifica nella quale i requisiti specificati sono adeguati a un utilizzo previsto"@it ;
    skos:notation "3.9" ;
    rdfs:subClassOf iso17025:Verification .

iso17025:DecisionRule a owl:Class ;
    rdfs:label "Regola Decisionale"@it ;
    rdfs:label "Decision Rule"@en ;
    rdfs:comment "Regola che descrive in che modo si tiene conto dell'incertezza di misura quando si dichiara la conformità a un requisito specificato"@it ;
    skos:notation "3.7" .

iso17025:InterlaboratoryComparison a owl:Class ;
    rdfs:label "Confronto Interlaboratorio"@it ;
    rdfs:label "Interlaboratory Comparison"@en ;
    rdfs:comment "Organizzazione, esecuzione e valutazione di misurazioni o prove sugli stessi oggetti o su oggetti simili da parte di due o più laboratori"@it ;
    skos:notation "3.3" .

iso17025:IntralaboratoryComparison a owl:Class ;
    rdfs:label "Confronto Intralaboratorio"@it ;
    rdfs:label "Intralaboratory Comparison"@en ;
    rdfs:comment "Organizzazione, esecuzione e valutazione di misurazioni o prove sugli stessi oggetti o su oggetti simili, nell'ambito dello stesso laboratorio"@it ;
    skos:notation "3.4" .

iso17025:ProficiencyTesting a owl:Class ;
    rdfs:label "Prova Valutativa Interlaboratorio"@it ;
    rdfs:label "Proficiency Testing"@en ;
    rdfs:comment "Valutazione delle prestazioni di un partecipante a fronte di criteri prestabiliti mediante confronti interlaboratorio"@it ;
    skos:notation "3.5" ;
    rdfs:subClassOf iso17025:InterlaboratoryComparison .

iso17025:Complaint a owl:Class ;
    rdfs:label "Reclamo"@it ;
    rdfs:label "Complaint"@en ;
    rdfs:comment "Espressione d'insoddisfazione manifestata da una persona o da una organizzazione ad un laboratorio relativa alle attività o ai risultati di tale laboratorio, per la quale è attesa una risposta"@it ;
    skos:notation "3.2" .

# ============================================================================
# CLAUSOLA 4: REQUISITI GENERALI
# ============================================================================

iso17025:Clause4_GeneralRequirements a owl:Class ;
    rdfs:label "Clausola 4 - Requisiti Generali"@it ;
    rdfs:label "Clause 4 - General Requirements"@en ;
    rdfs:subClassOf iso17025:CompetenceRequirement ;
    rdfs:comment "Requisiti generali per imparzialità e riservatezza"@it .

## 4.1 Imparzialità - Dettaglio Completo
iso17025:Impartiality a owl:Class ;
    rdfs:label "Imparzialità"@it ;
    rdfs:label "Impartiality"@en ;
    rdfs:subClassOf iso17025:Clause4_GeneralRequirements ;
    rdfs:comment "Presenza di obiettività"@it ;
    skos:notation "4.1" ;
    skos:definition "Obiettività significa che non esistono conflitti di interesse, o che questi sono risolti in modo da non influenzare negativamente le conseguenti attività del laboratorio"@it .

iso17025:ImpartialityManagement a owl:Class ;
    rdfs:label "Gestione dell'Imparzialità"@it ;
    rdfs:subClassOf iso17025:Impartiality ;
    skos:notation "4.1.1" ;
    rdfs:comment "Le attività del laboratorio devono essere effettuate in modo imparziale e strutturate e gestite in modo da salvaguardare l'imparzialità"@it .

iso17025:TopManagementCommitment a owl:Class ;
    rdfs:label "Impegno della Direzione"@it ;
    rdfs:subClassOf iso17025:Impartiality ;
    skos:notation "4.1.2" ;
    rdfs:comment "La direzione del laboratorio deve impegnarsi per l'imparzialità"@it .

iso17025:ImpartialityResponsibility a owl:Class ;
    rdfs:label "Responsabilità per l'Imparzialità"@it ;
    rdfs:subClassOf iso17025:Impartiality ;
    skos:notation "4.1.3" ;
    rdfs:comment "Il laboratorio deve essere responsabile dell'imparzialità delle proprie attività di laboratorio"@it .

iso17025:RiskToImpartiality a owl:Class ;
    rdfs:label "Rischio per l'Imparzialità"@it ;
    rdfs:label "Risk to Impartiality"@en ;
    rdfs:subClassOf iso17025:Impartiality ;
    skos:notation "4.1.4" ;
    rdfs:comment "Il laboratorio deve identificare su base continuativa i rischi per la propria imparzialità"@it .

iso17025:ImpartialityRiskSource a owl:Class ;
    rdfs:label "Fonte di Rischio per l'Imparzialità"@it ;
    rdfs:subClassOf iso17025:RiskToImpartiality ;
    rdfs:comment "Fonti di rischio per l'imparzialità incluse proprietà, governance, direzione, personale, risorse condivise, aspetti finanziari, contratti, marketing"@it .

iso17025:ImpartialityRiskMitigation a owl:Class ;
    rdfs:label "Mitigazione del Rischio per l'Imparzialità"@it ;
    rdfs:subClassOf iso17025:RiskToImpartiality ;
    skos:notation "4.1.5" ;
    rdfs:comment "Se viene identificato un rischio per l'imparzialità, il laboratorio deve essere in grado di dimostrare come elimina o minimizza tale rischio"@it .

## 4.2 Riservatezza - Dettaglio Completo
iso17025:Confidentiality a owl:Class ;
    rdfs:label "Riservatezza"@it ;
    rdfs:label "Confidentiality"@en ;
    rdfs:subClassOf iso17025:Clause4_GeneralRequirements ;
    skos:notation "4.2" ;
    rdfs:comment "Gestione di tutte le informazioni ottenute o generate nel corso dell'effettuazione delle attività di laboratorio"@it .

iso17025:LegallyEnforceableCommitment a owl:Class ;
    rdfs:label "Impegno Legalmente Vincolante"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    skos:notation "4.2.1" ;
    rdfs:comment "Il laboratorio deve essere responsabile, per mezzo di impegni legalmente vincolanti, della gestione di tutte le informazioni"@it .

iso17025:ProprietaryInformation a owl:Class ;
    rdfs:label "Informazioni Proprietarie"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    rdfs:comment "Tutte le informazioni non rese pubbliche dal cliente sono considerate informazioni proprietarie e riservate"@it .

iso17025:PublicDomainInformation a owl:Class ;
    rdfs:label "Informazioni di Pubblico Dominio"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    rdfs:comment "Informazioni che il laboratorio intende rendere di pubblico dominio con preavviso al cliente"@it .

iso17025:LegalDisclosure a owl:Class ;
    rdfs:label "Comunicazione per Obbligo di Legge"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    skos:notation "4.2.2" ;
    rdfs:comment "Quando al laboratorio è richiesto per legge di comunicare informazioni riservate"@it .

iso17025:ThirdPartyInformation a owl:Class ;
    rdfs:label "Informazioni da Terze Parti"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    skos:notation "4.2.3" ;
    rdfs:comment "Informazioni relative al cliente ottenute da fonti diverse dal cliente stesso"@it .

iso17025:PersonnelConfidentiality a owl:Class ;
    rdfs:label "Riservatezza del Personale"@it ;
    rdfs:subClassOf iso17025:Confidentiality ;
    skos:notation "4.2.4" ;
    rdfs:comment "Il personale deve mantenere riservate tutte le informazioni ottenute o generate"@it .

# ============================================================================
# CLAUSOLA 5: REQUISITI STRUTTURALI - ESPANSIONE COMPLETA
# ============================================================================

iso17025:Clause5_StructuralRequirements a owl:Class ;
    rdfs:label "Clausola 5 - Requisiti Strutturali"@it ;
    rdfs:label "Clause 5 - Structural Requirements"@en ;
    rdfs:subClassOf iso17025:CompetenceRequirement ;
    rdfs:comment "Requisiti per la struttura organizzativa e le responsabilità"@it .

## 5.1 Soggetto Giuridico
iso17025:LegalEntity a owl:Class ;
    rdfs:label "Soggetto Giuridico"@it ;
    rdfs:label "Legal Entity"@en ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    rdfs:subClassOf org:FormalOrganization ;
    skos:notation "5.1" ;
    rdfs:comment "Il laboratorio deve essere un soggetto giuridico, o una parte definita di esso"@it .

iso17025:PublicLaboratory a owl:Class ;
    rdfs:label "Laboratorio Pubblico"@it ;
    rdfs:subClassOf iso17025:LegalEntity ;
    rdfs:comment "Un laboratorio pubblico è considerato un soggetto giuridico sulla base della propria natura pubblica"@it .

iso17025:PrivateLaboratory a owl:Class ;
    rdfs:label "Laboratorio Privato"@it ;
    rdfs:subClassOf iso17025:LegalEntity ;
    rdfs:comment "Laboratorio operante come entità privata"@it .

## 5.2 Direzione del Laboratorio
iso17025:LaboratoryManagement a owl:Class ;
    rdfs:label "Direzione del Laboratorio"@it ;
    rdfs:label "Laboratory Management"@en ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.2" ;
    rdfs:comment "Il laboratorio deve identificare la direzione che ha la responsabilità complessiva del laboratorio"@it .

iso17025:OverallResponsibility a owl:Class ;
    rdfs:label "Responsabilità Complessiva"@it ;
    rdfs:subClassOf iso17025:LaboratoryManagement ;
    rdfs:comment "Responsabilità complessiva per il laboratorio"@it .

## 5.3 Gamma di Attività
iso17025:ScopeOfActivities a owl:Class ;
    rdfs:label "Gamma delle Attività"@it ;
    rdfs:label "Scope of Activities"@en ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.3" ;
    rdfs:comment "Il laboratorio deve definire e documentare la gamma delle proprie attività di laboratorio"@it .

iso17025:DocumentedScope a owl:Class ;
    rdfs:label "Campo di Applicazione Documentato"@it ;
    rdfs:subClassOf iso17025:ScopeOfActivities ;
    rdfs:comment "Campo di applicazione documentato per cui il laboratorio opera in conformità"@it .

iso17025:ExcludedActivities a owl:Class ;
    rdfs:label "Attività Escluse"@it ;
    rdfs:subClassOf iso17025:ScopeOfActivities ;
    rdfs:comment "Attività fornite dall'esterno su base continuativa sono escluse"@it .

## 5.4 Conformità delle Attività
iso17025:ActivityCompliance a owl:Class ;
    rdfs:label "Conformità delle Attività"@it ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.4" ;
    rdfs:comment "Le attività devono essere conformi ai requisiti del presente documento, dei clienti, delle autorità e delle organizzazioni"@it .

iso17025:PermanentFacility a owl:Class ;
    rdfs:label "Struttura Permanente"@it ;
    rdfs:subClassOf iso17025:ActivityCompliance ;
    rdfs:comment "Attività eseguite presso le strutture permanenti del laboratorio"@it .

iso17025:TemporarySite a owl:Class ;
    rdfs:label "Sito Temporaneo"@it ;
    rdfs:subClassOf iso17025:ActivityCompliance ;
    rdfs:comment "Attività eseguite presso sedi temporanee o mobili"@it .

iso17025:CustomerSite a owl:Class ;
    rdfs:label "Sede del Cliente"@it ;
    rdfs:subClassOf iso17025:ActivityCompliance ;
    rdfs:comment "Attività eseguite presso le strutture dei clienti"@it .

## 5.5 Struttura Organizzativa
iso17025:OrganizationalStructure a owl:Class ;
    rdfs:label "Struttura Organizzativa"@it ;
    rdfs:label "Organizational Structure"@en ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.5" ;
    rdfs:comment "Il laboratorio deve definire la propria organizzazione e struttura direttiva"@it .

iso17025:ManagementStructure a owl:Class ;
    rdfs:label "Struttura Direttiva"@it ;
    rdfs:subClassOf iso17025:OrganizationalStructure ;
    skos:notation "5.5.a" ;
    rdfs:comment "Definizione della struttura direttiva e collocazione nell'organizzazione"@it .

iso17025:ResponsibilityAuthority a owl:Class ;
    rdfs:label "Responsabilità e Autorità"@it ;
    rdfs:subClassOf iso17025:OrganizationalStructure ;
    skos:notation "5.5.b" ;
    rdfs:comment "Specificare responsabilità, autorità e interazioni del personale"@it .

iso17025:DocumentedProcedures a owl:Class ;
    rdfs:label "Procedure Documentate"@it ;
    rdfs:subClassOf iso17025:OrganizationalStructure ;
    skos:notation "5.5.c" ;
    rdfs:comment "Documentare le procedure per assicurare esecuzione regolare e coerente"@it .

## 5.6 Personale con Autorità
iso17025:AuthorizedPersonnel a owl:Class ;
    rdfs:label "Personale con Autorità"@it ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.6" ;
    rdfs:comment "Personale con autorità e risorse necessarie per eseguire i propri compiti"@it .

iso17025:SystemImplementation a owl:Class ;
    rdfs:label "Attuazione del Sistema"@it ;
    rdfs:subClassOf iso17025:AuthorizedPersonnel ;
    skos:notation "5.6.a" ;
    rdfs:comment "Attuazione, mantenimento e miglioramento del sistema di gestione"@it .

iso17025:DeviationIdentification a owl:Class ;
    rdfs:label "Identificazione Scostamenti"@it ;
    rdfs:subClassOf iso17025:AuthorizedPersonnel ;
    skos:notation "5.6.b" ;
    rdfs:comment "Identificazione di scostamenti dal sistema di gestione"@it .

iso17025:PreventiveAction a owl:Class ;
    rdfs:label "Azione Preventiva"@it ;
    rdfs:subClassOf iso17025:AuthorizedPersonnel ;
    skos:notation "5.6.c" ;
    rdfs:comment "Avvio di azioni per prevenire o minimizzare scostamenti"@it .

iso17025:ManagementReporting a owl:Class ;
    rdfs:label "Reporting alla Direzione"@it ;
    rdfs:subClassOf iso17025:AuthorizedPersonnel ;
    skos:notation "5.6.d" ;
    rdfs:comment "Riferire alla direzione circa le prestazioni del sistema"@it .

iso17025:ActivityEffectiveness a owl:Class ;
    rdfs:label "Efficacia delle Attività"@it ;
    rdfs:subClassOf iso17025:AuthorizedPersonnel ;
    skos:notation "5.6.e" ;
    rdfs:comment "Assicurare l'efficacia delle attività di laboratorio"@it .

## 5.7 Comunicazione della Direzione
iso17025:ManagementCommunication a owl:Class ;
    rdfs:label "Comunicazione della Direzione"@it ;
    rdfs:subClassOf iso17025:Clause5_StructuralRequirements ;
    skos:notation "5.7" ;
    rdfs:comment "La direzione del laboratorio deve assicurare le comunicazioni"@it .

iso17025:EffectivenessCommunication a owl:Class ;
    rdfs:label "Comunicazione sull'Efficacia"@it ;
    rdfs:subClassOf iso17025:ManagementCommunication ;
    skos:notation "5.7.a" ;
    rdfs:comment "Comunicazioni relative all'efficacia del sistema di gestione"@it .

iso17025:SystemIntegrity a owl:Class ;
    rdfs:label "Integrità del Sistema"@it ;
    rdfs:subClassOf iso17025:ManagementCommunication ;
    skos:notation "5.7.b" ;
    rdfs:comment "Mantenimento dell'integrità del sistema quando vengono pianificate modifiche"@it .

# ============================================================================
# CLAUSOLA 6: REQUISITI RELATIVI ALLE RISORSE - ESPANSIONE COMPLETA
# ============================================================================

iso17025:Clause6_ResourceRequirements a owl:Class ;
    rdfs:label "Clausola 6 - Requisiti relativi alle Risorse"@it ;
    rdfs:label "Clause 6 - Resource Requirements"@en ;
    rdfs:subClassOf iso17025:CompetenceRequirement ;
    rdfs:comment "Requisiti per personale, strutture, dotazioni, sistemi e servizi di supporto"@it .

## 6.1 Generalità
iso17025:ResourceGeneralRequirements a owl:Class ;
    rdfs:label "Requisiti Generali delle Risorse"@it ;
    rdfs:subClassOf iso17025:Clause6_ResourceRequirements ;
    skos:notation "6.1" ;
    rdfs:comment "Il laboratorio deve disporre del personale, delle strutture, delle dotazioni, dei sistemi e dei servizi di supporto necessari"@it .

## 6.2 Personale - Dettaglio Completo
iso17025:Personnel a owl:Class ;
    rdfs:label "Personale"@it ;
    rdfs:label "Personnel"@en ;
    rdfs:subClassOf iso17025:Clause6_ResourceRequirements ;
    rdfs:subClassOf foaf:Person ;
    skos:notation "6.2" ;
    rdfs:comment "Tutto il personale del laboratorio che potrebbe influenzare le attività"@it .

iso17025:PersonnelImpartiality a owl:Class ;
    rdfs:label "Imparzialità del Personale"@it ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.1" ;
    rdfs:comment "Tutto il personale deve agire con imparzialità"@it .

iso17025:PersonnelCompetence a owl:Class ;
    rdfs:label "Competenza del Personale"@it ;
    rdfs:label "Personnel Competence"@en ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.2" ;
    rdfs:comment "Il laboratorio deve documentare i requisiti di competenza per ogni funzione"@it .

iso17025:CompetenceRequirementsDetail a owl:Class ;
    rdfs:label "Dettagli Requisiti di Competenza"@it ;
    rdfs:subClassOf iso17025:PersonnelCompetence ;
    rdfs:comment "Requisiti di istruzione, qualifica, formazione, conoscenza tecnica, abilità ed esperienza"@it .

iso17025:PersonnelEvaluation a owl:Class ;
    rdfs:label "Valutazione del Personale"@it ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.3" ;
    rdfs:comment "Assicurare che il personale abbia la competenza per eseguire le attività"@it .

iso17025:PersonnelCommunication a owl:Class ;
    rdfs:label "Comunicazione al Personale"@it ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.4" ;
    rdfs:comment "La direzione deve comunicare al personale i relativi compiti, responsabilità ed autorità"@it .

iso17025:PersonnelManagementProcedure a owl:Class ;
    rdfs:label "Procedura Gestione Personale"@it ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.5" ;
    rdfs:comment "Il laboratorio deve disporre di procedure per la gestione del personale"@it .

iso17025:CompetenceDetermination a owl:Class ;
    rdfs:label "Determinazione Requisiti Competenza"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.a" .

iso17025:PersonnelSelection a owl:Class ;
    rdfs:label "Selezione del Personale"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.b" .

iso17025:PersonnelTraining a owl:Class ;
    rdfs:label "Formazione del Personale"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.c" .

iso17025:PersonnelSupervision a owl:Class ;
    rdfs:label "Supervisione del Personale"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.d" .

iso17025:PersonnelAuthorization a owl:Class ;
    rdfs:label "Autorizzazione del Personale"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.e" .

iso17025:CompetenceMonitoring a owl:Class ;
    rdfs:label "Monitoraggio della Competenza"@it ;
    rdfs:subClassOf iso17025:PersonnelManagementProcedure ;
    skos:notation "6.2.5.f" .

iso17025:SpecificActivityAuthorization a owl:Class ;
    rdfs:label "Autorizzazione Attività Specifiche"@it ;
    rdfs:subClassOf iso17025:Personnel ;
    skos:notation "6.2.6" ;
    rdfs:comment "Il laboratorio deve autorizzare personale che esegua specifiche attività"@it .

iso17025:MethodDevelopmentAuthorization a owl:Class ;
    rdfs:label "Autorizzazione Sviluppo Metodi"@it ;
    rdfs:subClassOf iso17025:SpecificActivityAuthorization ;
    skos:notation "6.2.6.a" ;
    rdfs:comment "Sviluppo, modifica, verifica e validazione dei metodi"@it .

iso17025:ResultAnalysisAuthorization a owl:Class ;
    rdfs:label "Autorizzazione Analisi Risultati"@it ;
    rdfs:subClassOf iso17025:SpecificActivityAuthorization ;
    skos:notation "6.2.6.b" ;
    rdfs:comment "Analisi dei risultati, compresa la dichiarazione di conformità"@it .

iso17025:ReportApprovalAuthorization a owl:Class ;
    rdfs:label "Autorizzazione Approvazione Rapporti"@it ;
    rdfs:subClassOf iso17025:SpecificActivityAuthorization ;
    skos:notation "6.2.6.c" ;
    rdfs:comment "Presentazione, riesame e approvazione dei risultati"@it .

## 6.3 Strutture e Condizioni Ambientali - Dettaglio Completo
iso17025:Facilities a owl:Class ;
